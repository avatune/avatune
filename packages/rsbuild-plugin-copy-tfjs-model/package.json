{
  "name": "@avatune/rsbuild-plugin-copy-tfjs-model",
  "version": "0.0.0",
  "type": "module",
  "description": "Rsbuild plugin to copy TensorFlow.js models during build",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@avatune/typescript-config": "workspace:*",
    "@rsbuild/core": "^1.5.17",
    "@rslib/core": "^0.16.1",
    "@types/node": "^22.18.12",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@rsbuild/core": ">=1.0.0"
  }
}
