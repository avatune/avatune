{
  "name": "@avatune/rsbuild-plugin-svg-to-svelte",
  "version": "0.0.0",
  "type": "module",
  "description": "Rsbuild plugin to transform SVG files into Svelte components",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0"
  },
  "devDependencies": {
    "@avatune/typescript-config": "workspace:*",
    "@rslib/core": "^0.16.1",
    "@types/node": "^22.18.12",
    "deepmerge": "^4.3.1",
    "svelte": "^5.0.0",
    "svgo": "^4.0.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@rsbuild/core": ">=1.0.0",
    "svelte": ">=5.0.0"
  }
}
